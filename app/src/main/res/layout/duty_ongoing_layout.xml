<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.cardview.widget.CardView
            android:id="@+id/cvOngoingDuty"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_5sdp"
            android:elevation="@dimen/_5sdp"
            app:cardCornerRadius="@dimen/_10sdp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <androidx.cardview.widget.CardView
                    android:id="@+id/cvProfileOnGoingImg"
                    android:layout_width="@dimen/_50sdp"
                    android:layout_height="@dimen/_50sdp"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:layout_marginTop="@dimen/_15sdp"
                    android:padding="@dimen/_5sdp"
                    app:cardCornerRadius="@dimen/_50sdp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"

                    >

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/ivProfileHomeImg"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="centerCrop"
                        android:src="@drawable/verify_img" />
                </androidx.cardview.widget.CardView>

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tvOngoingProfileName"
                    style="@style/TextStyle1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:fontFamily="@font/inter_bold"
                    android:text="@string/gajendra"
                    android:textColor="@color/default_text_color"
                    app:layout_constraintStart_toEndOf="@+id/cvProfileOnGoingImg"
                    app:layout_constraintTop_toTopOf="@+id/cvProfileOnGoingImg" />


                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tvEnterTruckStatusDOngoing"
                    style="@style/TextStyle1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5sdp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:text="@string/open"
                    android:textColor="@color/seller_color"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/cvProfileOnGoingImg" />


                <androidx.appcompat.widget.LinearLayoutCompat
                    android:id="@+id/llcTuckNumberDOngoing"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toEndOf="@+id/cvProfileOnGoingImg"
                    app:layout_constraintTop_toBottomOf="@+id/tvOngoingProfileName">

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tvEnterTruckNumberText"
                        style="@style/defaultTextStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:text="@string/enter_truck_no"
                        android:textColor="@color/default_text_color"
                        app:layout_constraintStart_toEndOf="@+id/cvProfileOnGoingImg"
                        app:layout_constraintTop_toTopOf="@+id/cvProfileOnGoingImg" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tvEnterTruckNumber"
                        style="@style/defaultTextStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:text="@string/up16cm1912"
                        android:textColor="@color/default_text_color"
                        app:layout_constraintStart_toEndOf="@+id/cvProfileOnGoingImg"
                        app:layout_constraintTop_toTopOf="@+id/cvProfileOnGoingImg" />

                </androidx.appcompat.widget.LinearLayoutCompat>

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:id="@+id/llcTuckCompanyDOngoing"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toEndOf="@+id/cvProfileOnGoingImg"
                    app:layout_constraintTop_toBottomOf="@+id/llcTuckNumberDOngoing">

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tvEnterTruckCompanyText"
                        style="@style/defaultTextStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:text="@string/truck_company"
                        android:textColor="@color/default_text_color"
                        app:layout_constraintStart_toEndOf="@+id/cvProfileOnGoingImg"
                        app:layout_constraintTop_toTopOf="@+id/cvProfileOnGoingImg" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tvEnterTruckCompany"
                        style="@style/defaultTextStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:text="@string/xyz_company"
                        android:textColor="@color/default_text_color"
                        app:layout_constraintStart_toEndOf="@+id/cvProfileOnGoingImg"
                        app:layout_constraintTop_toTopOf="@+id/cvProfileOnGoingImg" />

                </androidx.appcompat.widget.LinearLayoutCompat>

                <View
                    android:id="@+id/separateLine1DO"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@color/box_color_2"
                    app:layout_constraintTop_toBottomOf="@id/cvProfileOnGoingImg" />

                <View
                    android:id="@+id/viewTrackStatus1"
                    android:layout_width="@dimen/_20sdp"
                    android:layout_height="@dimen/_20sdp"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/ic_track_status1"
                    android:elevation="@dimen/_5sdp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/separateLine1DO" />

                <View
                    android:id="@+id/viewTrackStatus1Vertical"
                    android:layout_width="@dimen/_1sdp"
                    android:layout_height="@dimen/_50sdp"
                    android:background="@drawable/vertical_line"
                    app:layout_constraintEnd_toEndOf="@+id/viewTrackStatus1"
                    app:layout_constraintStart_toStartOf="@+id/viewTrackStatus1"
                    app:layout_constraintTop_toBottomOf="@+id/viewTrackStatus1" />

                <View
                    android:id="@+id/viewTrackStatus1Horizontal"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_1sdp"
                    android:background="@color/box_color_2"
                    app:layout_constraintBottom_toBottomOf="@+id/viewTrackStatus1Vertical"
                    app:layout_constraintEnd_toStartOf="@+id/cvTrackStatus3"
                    app:layout_constraintStart_toEndOf="@+id/viewTrackStatus1Vertical"
                    app:layout_constraintTop_toTopOf="@+id/viewTrackStatus1Vertical" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/cvTrackStatus3"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_marginEnd="@dimen/_15sdp"
                    android:src="@drawable/ic_google_map"
                    app:cardCornerRadius="@dimen/_50sdp"
                    app:layout_constraintBottom_toBottomOf="@+id/viewTrackStatus1Horizontal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/viewTrackStatus1Horizontal"
                    app:layout_constraintTop_toTopOf="@+id/viewTrackStatus1Horizontal">

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/viewTrackStatus3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:src="@drawable/ic_google_map" />
                </androidx.cardview.widget.CardView>


                <View
                    android:id="@+id/viewTrackStatus2"
                    android:layout_width="@dimen/_20sdp"
                    android:layout_height="@dimen/_20sdp"
                    android:background="@drawable/ic_map_pin"
                    app:layout_constraintEnd_toEndOf="@+id/viewTrackStatus1"
                    app:layout_constraintStart_toStartOf="@+id/viewTrackStatus1"
                    app:layout_constraintTop_toBottomOf="@+id/viewTrackStatus1Vertical" />


                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tvTrack1StatusDate"
                    style="@style/defaultTextStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:text="@string/sun_feb_19_04_14_pm"
                    android:textColor="@color/default_text_color"
                    app:layout_constraintStart_toEndOf="@+id/viewTrackStatus1"
                    app:layout_constraintTop_toTopOf="@+id/viewTrackStatus1" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tvTrack1StatusLocation"
                    style="@style/defaultTextStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:fontFamily="@font/inter_bold"
                    android:text="@string/odunsi_junction_kiosk_lane"
                    android:textColor="@color/default_text_color"
                    app:layout_constraintStart_toEndOf="@+id/viewTrackStatus1"
                    app:layout_constraintTop_toBottomOf="@+id/tvTrack1StatusDate" />


                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tvTrack2StatusDate"
                    style="@style/defaultTextStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:text="@string/sun_feb_19_04_14_pm"
                    android:textColor="@color/default_text_color"
                    app:layout_constraintStart_toEndOf="@+id/viewTrackStatus2"
                    app:layout_constraintTop_toTopOf="@+id/viewTrackStatus2" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tvTrack2StatusLocation"
                    style="@style/defaultTextStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:fontFamily="@font/inter_bold"
                    android:text="@string/odunsi_junction_kiosk_lane"
                    android:textColor="@color/default_text_color"
                    app:layout_constraintStart_toEndOf="@+id/viewTrackStatus2"
                    app:layout_constraintTop_toBottomOf="@+id/tvTrack2StatusDate" />

                <View
                    android:id="@+id/separateLine2DO"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@color/box_color_2"
                    app:layout_constraintTop_toBottomOf="@id/tvTrack2StatusLocation" />


                <GridLayout
                    android:id="@+id/gridLayoutDO"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_15sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_15sdp"
                    android:layout_marginBottom="@dimen/_10sdp"
                    android:columnCount="2"
                    android:rowCount="2"
                    app:layout_constraintTop_toBottomOf="@+id/separateLine2DO">

                    <CheckBox
                        android:id="@+id/cbPollution"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_row="0"
                        android:layout_column="0"
                        android:layout_columnWeight="1"
                        android:text="@string/pollution" />

                    <CheckBox
                        android:id="@+id/cbPermit"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_row="0"
                        android:layout_column="1"
                        android:layout_columnWeight="1"
                        android:text="@string/permit" />

                    <CheckBox
                        android:id="@+id/cbRC"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_row="1"
                        android:layout_column="0"
                        android:layout_columnWeight="1"
                        android:text="@string/rc" />

                    <CheckBox
                        android:id="@+id/cbEwayBill"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_row="1"
                        android:layout_column="1"
                        android:layout_columnWeight="1"
                        android:text="@string/eway_bill" />


                </GridLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>


        <androidx.appcompat.widget.LinearLayoutCompat
            android:id="@+id/llcDOngoinhBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_15sdp"
            android:layout_marginTop="@dimen/_20sdp"
            android:layout_marginEnd="@dimen/_15sdp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cvOngoingDuty">

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvAddImgBtn"
                style="@style/defaultBtnStyle"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/seller_btn_drawable"
                android:drawableStart="@drawable/ic_call"
                android:gravity="center"
                android:text="@string/sos" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvHalfDayBtnAL"
                style="@style/Home2BuyerBtnStyle"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_15sdp"
                android:layout_weight="1"
                android:background="@drawable/default_gradient_btn_rounded_drawable"
                android:drawableStart="@drawable/ic_call"
                android:gravity="center"
                android:text="@string/call_lawyer" />
        </androidx.appcompat.widget.LinearLayoutCompat>

        <androidx.cardview.widget.CardView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            app:cardCornerRadius="@dimen/_10sdp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/llcDOngoinhBtn">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/_10sdp"
                >

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/ivTruckDO"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:src="@drawable/truck_img"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tvOngoingTuckDateDo"
                    style="@style/defaultTextStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:fontFamily="@font/inter_bold"
                    android:text="@string/sun_feb_19_04_14_pm"
                    android:textColor="@color/black"
                    app:layout_constraintStart_toEndOf="@+id/ivTruckDO"
                    app:layout_constraintTop_toTopOf="@+id/ivTruckDO" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tvTruckDOngoingStatus"
                    style="@style/TextStyle1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5sdp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:text="@string/open"
                    android:textColor="@color/seller_color"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/ivTruckDO" />


                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tvTrackStatusInfoDO"
                    style="@style/defaultTextStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:text="@string/truck_info"
                    android:textColor="@color/default_text_color"
                    app:layout_constraintStart_toStartOf="@+id/tvOngoingTuckDateDo"
                    app:layout_constraintTop_toBottomOf="@+id/tvOngoingTuckDateDo" />


            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.cardview.widget.CardView>


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>